{
  "compileOnSave": true,
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "target": "es2020",
    "lib": [ "es2020", "dom" ],
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "noLib": false,
    "rootDir": "ts",
    "outDir": "js",
    "types": [ "vite/client" ]
  },
  "include": [ "ts" ]
}